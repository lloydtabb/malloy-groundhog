source: _groundhogs is table('duckdb:data/groundhogs.csv')
extend {
    primary_key: the_day
    accept: the_day, lug, shortname, city, region,
            isGroundhog,  shadow, country
    rename: saw_shadow is shadow
}

query: groundhogs is _groundhogs -> {
    where: country = 'USA'
}

